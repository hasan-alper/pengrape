<%- contentFor('option1') %>
<section id="option-1-content">
	<div id="option-1-1-content">
		<div class="header-contents">
			<p class="header">Format</p>
			<p class="description">Set a date format.</p>
		</div>
		<div class="input-contents">
			<div class="input-content">
				<input type="text" id="input-custom-format" class="form-control" placeholder="yyyy-mm-dd" />
			</div>
		</div>
	</div>
</section>

<%- contentFor('sidebar1') %>
<p class="header">Random Date</p>
<p class="description">Generate a date between the two dates that you chose.</p>

<%- contentFor('sidebar2') %>
<p class="header">Range</p>
<p class="description">Enter start date and end date.</p>
<div class="input-contents">
	<div class="input-content min-max-content">
		<label>Start</label>
		<input type="number" class="form-control" id="input-start-year" value="2021" />
		<input type="number" class="form-control" id="input-start-month" value="1" />
		<input type="number" class="form-control" id="input-start-day" value="1" />
	</div>
	<div class="input-content min-max-content">
		<label>End</label>
		<input type="number" class="form-control" id="input-end-year" value="2022" />
		<input type="number" class="form-control" id="input-end-month" value="1" />
		<input type="number" class="form-control" id="input-end-day" value="1" />
	</div>
</div>

