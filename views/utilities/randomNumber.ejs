<% layout("layouts/boilerplate") %>
<label for="min-input">min:</label>
<input type="number" id="min-input" value="0" />
<label for="max-label">max:</label>
<input type="number" id="max-input" value="20" /><br />
<input type="radio" id="integer" name="type" value="integer" checked />
<label for="integer">Integer</label>
<input type="radio" id="decimal" name="type" value="decimal" />
<label for="decimal">Decimal</label>
<label for="precision">Precision</label>
<input type="number" id="precision" value="4" />

<p id="output">0</p>
<button id="button">Generate</button>
<button id="copy-button">Copy</button>
<a href="/">Home</a>
